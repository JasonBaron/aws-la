{
  "description" : "Ingest pipeline for AWS ELB Access Logs",
  "processors" : [
    {
      "grok": {
        "field": "message",
        "patterns":
          [
            "^%{TIMESTAMP_ISO8601:timestamp} %{DATA:elb_node} (?:-|%{IPV4:client_ip_adress}:%{NUMBER:client_port}) (?:-|%{IPV4:backend_ip_adress}:%{NUMBER:backend_port}) (?:-1|%{BASE10NUM:request_processing_time}) (?:-1|%{BASE10NUM:backend_processing_time}) (?:-1|%{BASE10NUM:response_processing_time}) (?:-|%{DATA:elb_status_code:int}) (?:-|%{DATA:backend_status_code:int}) %{NONNEGINT:received_bytes} %{NONNEGINT:sent_bytes} (?:\"- - - \"|%{QS:request}) (?:-|%{QS:agent}) (?:-|%{DATA:ssl_cipher}) (?:-|%{DATA:ssl_protocol})$"
          ]
      }
    },
    {
      "gsub": {
        "field": "request",
        "pattern": "\"",
        "replacement": "",
        "ignore_failure" : true
      }
    },
    {
      "gsub": {
        "field": "agent",
        "pattern": "\"",
        "replacement": "",
        "ignore_failure" : true
      }
    },
    {
      "grok": {
        "field": "request",
        "ignore_missing": true,
        "patterns":
          [
            "^%{WORD:request_verb} %{GREEDYDATA:request_url} HTTP/%{DATA:request_proto}$"
          ]
      }
    },
    {
      "geoip": {
        "field": "client_ip_adress",
        "ignore_missing": true
      }
    },
    {
      "user_agent": {
        "field": "agent",
        "ignore_missing": true
      }
    }
  ]
}
